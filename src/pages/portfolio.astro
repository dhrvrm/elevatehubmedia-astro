---
import Layout from '../layouts/Layout.astro';

import PortfolioHeroImage from "../assets/images/illustration-desk-top.webp";
import GiftIcon from "../assets/icons/gift-dynamic-gradient.png";


---

<Layout title="Disover Our Craft | ElevateHubMedia">
	<main>
		<section id="porfolio-hero">
		  <div class="porfolio-hero-text">
			<h1>See<span class="text-w-color"> Our Creations</span></h1>
			<p>
			  Dive into our world of animation! From simple sketches to full-blown
			  stories, we've turned ideas into visual treats. Check out our
			  portfolio to see how we bring stories to life.
			</p>
		  </div>
		  <div class="porfolio-hero-image">
			<picture class="hero-display-image">
			  
			  <img
				src={PortfolioHeroImage.src}
				loading="eager"
				alt="Know more about our portfolio in Animation Video, with great scripts, voice-overs, music, and animation."
			  />
			</picture>
		  </div>
		</section>
  
		<section id="porfolio-types">
		  <div class="porfolio-container">
			<h2>Whiteboard Wonders</h2>
			<p>
			  Classic and clear, our whiteboard animations break down complex
			  ideas into fun, easy-to-understand drawings.
			</p>
			<div class="porfolio-grid">
			  <div class="porfolio-item">
				<iframe
				  loading="lazy"
				  title="Whiteboard"
				  src="https://player.vimeo.com/video/779836194?h=18d23dc7f7&amp;dnt=1&amp;app_id=122963"
				  frameborder="0"
				  allow="autoplay; fullscreen; picture-in-picture; clipboard-write"
				  name="fitvid7"
				></iframe>
				<h3>Business Warriors</h3>
			  </div>
			  <div class="porfolio-item">
				<iframe
				  loading="lazy"
				  title="Whiteboard"
				  src="https://player.vimeo.com/video/779835400?h=46fce4db49&amp;dnt=1&amp;app_id=122963"
				  frameborder="0"
				  allow="autoplay; fullscreen; picture-in-picture; clipboard-write"
				  name="fitvid7"
				></iframe>
				<h3>Conqueri</h3>
			  </div>
